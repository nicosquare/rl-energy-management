    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "env": {
                "PYTHONPATH": "${workspaceRoot}"
            },
            "args": [
                // "--disable_logging"
            ],
            "justMyCode": false
        },
        {
            "name": "Debug 3rd party code",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "env": {
                "PYTHONPATH": "${workspaceFolder}"
            },
            "args": [
                // "--disable_logging",
                "-gpu",
                // "--random_starting_step",
                // "--central_agent",
                // "--encoding",
                // "--extended_observation", 
                "--epsilon=0.001",
                "-bs=8",
                "-g=1",
                "-alr=0.0001",
                "-clr=0.001",
                "-ann=64",
                "-cnn=64",
                "-rs=24",
                "-ts=1500",
            ],
            "justMyCode": false,
        }
    ]
}